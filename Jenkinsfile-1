
pipeline {
  agent { label 'linux' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Set mvnw permissions'){
      steps{
        sh 'chmod +x mvnw'
      }
    }
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') {
          sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar'
        }
      }
    }
  }
}
